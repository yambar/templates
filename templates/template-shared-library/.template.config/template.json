{
  "$schema": "http://json.schemastore.org/template",
  "author": "yambar",
  "classifications": [
    "Library"
  ],
  "name": "yambar empty shared library",
  "identity": "yambar.Templates.SharedLibrary",
  "shortName": "yambar-shared-library",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "TemplateShared",
  "preferNameDirectory": true,
  "symbols": {
    "Framework": {
      "type": "parameter",
      "description": "The target framework for the project.",
      "datatype": "choice",
      "choices": [
        {
          "choice": "net6.0",
          "displayName": ".NET 6.0"
        },
        {
          "choice": "netstandard2.1",
          "displayName": ".NET Standard 2.1"
        }
      ],
      "defaultValue": "net6.0",
      "replaces": "{TargetFramework}"
    }
  }
}
